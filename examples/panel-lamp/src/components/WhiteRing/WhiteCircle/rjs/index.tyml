<view style="{{ containerStyle }}">
    <canvas
        class="canvasWrapper"
        type="2d"
        canvas-id="{{ canvasId }}"
        style="width: {{ radius * 2 }}px; height:  {{ radius * 2 }}px;"
    >
    </canvas>
     <canvas
            class="canvasThumbWrapper"
            type="2d"
            canvas-id="{{ canvasId }}_thumb"
            style="width: {{ radius * 2 }}px; height:  {{ radius * 2 }}px;"
        ></canvas>
</view>
